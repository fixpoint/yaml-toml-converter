    "@nuxtjs/vuetify": "^1.0.0",
    "@types/js-yaml": "^3.12.3",
    "babel-eslint": "^10.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "push-dir": "^0.4.1",
