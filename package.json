{
  "name": "yaml-toml-converter",
  "version": "1.0.0",
  "description": "YAML/TOML converter",
  "author": "Fixpoint, Inc.",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
